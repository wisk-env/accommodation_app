<h1>登録済み一覧</h1>

<table>
  <thead>
    <tr>
      <th>画像</th>
      <th>施設名</th>
      <th>紹介文</th>
      <th>料金</th>
      <th>作成日</th>
      <th>投稿者(仮User_id)</th>
    </tr>
  </thead>
  <tbody>
    <% @rooms.each do |room| %>
      <tr>
        <td><%= "施設画像" %></td>
        <td><%= link_to room.room_name, room_path(room) %></td>
        <td><%= room.room_info %></td>
        <td><%= "¥" + room.fee.to_s + "~/ 日" %></td>
        <td><%= room.created_at %></td>
        <td><%= room.user_id %></td>
      </tr>
    <% end %>
  </tbody>
</table>
